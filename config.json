{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "Even-Fibonacci-numbers",
      "Fifth smallest element in array",
      "Largest _palindrome_product",
      "Largest_Prime_Factor",
      "multiple_of_3_and_5"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/",
    "template": "node_modules/better-docs/"
  }
}
